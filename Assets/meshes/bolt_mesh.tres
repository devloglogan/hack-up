[sub_resource type="Shader" id="Shader_4fuv8"]
code = "shader_type spatial;
render_mode cull_disabled;

uniform sampler2D bolt_texture: source_color;
uniform vec3 emission_color: source_color;

varying float lifetime;

void vertex() {
	lifetime = INSTANCE_CUSTOM.y;
}

void fragment() {
	float bolt = texture(bolt_texture, UV - vec2(TIME * .5, 0.)).r;
	EMISSION = vec3(bolt) * emission_color * 2.;
	ALPHA = bolt * smoothstep(.5, 0, abs(UV.x-.5)) * smoothstep(.5, 0., abs(lifetime-.5));
}
"

